<div>
<div id="ButtonsInUsers" style="padding: 16px;">
	<button class="btn btn-default" onclick="addUser()" data-toggle="modal" data-target="#idAdduserModal"><i class="fa fa-plus" ></i> Add user</button>
	<button class="btn btn-default" onclick="uploadUsers()" ><i class="fa fa-refresh" ></i> Refresh</button>

</div>

<div class="container bootstrap snippet">
    <div class="row">
        <div class="col-lg-12">
            <div class="main-box no-header clearfix">
                <div class="main-box-body clearfix">
                    <div class="table-responsive">
                        <table class="table user-list">
                            <thead>
                                <tr>
                                <th><span>User</span></th>
                                <th><span>Id</span></th>
                                <!--<th class="text-center"><span>Type</span></th>-->
								<th><span>Password</span></th>
								<th><span>Branch</span></th>
                                <!--<th><span>Email</span></th>-->
                                <th>&nbsp;</th>
                                </tr>
                            </thead>
                            <tbody>
							<% Users.forEach(function(user) { %>
                                <tr>
                                    <td>
                                        <img src="https://bootdey.com/img/Content/user_1.jpg" alt="">
                                        <p class="user-link user-name"><%= user.name %></p>
                                        <span class="user-subhead"> <%= user.type %></span>
                                    </td>
                                    <td><%= user.id %></td>
                                    <td><%= user.password1 %></td>
									<% if(user.type==="employee"){%>
										<td><%= user.numOfBranch %></td>
									<% }else{%>
										<td></td>
									<% } %>
                                    <!--<td class="text-center">
                                        <span class="label label-default">pending</span>
                                    </td>
                                    <td>
                                        <a href="#">marlon@brando.com</a>
                                    </td>-->
                                   <td style="width: 20%;">
                                        <div id= <%= user.id %>  onclick="updateUser(this)" data-toggle="modal" data-target="#idUpdateuserModal" class="table-link">
                                            <span class="fa-stack">
												<i class="fa fa-square fa-stack-2x"></i>
                                                <i class="fa fa-edit fa-stack-1x fa-inverse"></i>
                                            </span>
                                        </div>
                                        <div id= <%= user.id %> onclick="removeUser(this)"  class="table-link danger">
                                            <span class="fa-stack">
												<i class="fa fa-square fa-stack-2x"></i>
                                                <i class="fa fa-trash fa-stack-1x fa-inverse"></i>                                            </span>
                                        </div>
                                    </td>
                                </tr>
                                <% }); %>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

