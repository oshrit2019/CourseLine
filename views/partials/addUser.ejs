
<div id="idAdduserModal" class="modal fade"  tabindex="-1" role="dialog" aria-labelledby="myModalLabel"
  aria-hidden="true">
  <div class="modal-dialog" role="document">
	<div class="modal-content" >
		<div class="modal-header text-center">
			<h4 class="modal-title w-100 font-weight-bold">Add user</h4>
			<button id="closeModalBtn" type="button" class="close" data-dismiss="modal" aria-label="Close">
				<span aria-hidden="true">&times;</span>
			</button>
        </div>
	<div class="modal-body mx-3">
	  <div class="md-form mb-5">
	    <i class="fas fa-user prefix grey-text"></i>
		<input type="text" id="userName1" name="uname" class="form-control validate" required>
		<label for="uname" data-error="wrong" data-success="right"  >Username</label>
	  </div>
	   <div class="md-form mb-5">
	    <i class="fas fa-id-card prefix grey-text"></i>
		<input type="text" id="id1" name="id" class="form-control validate" required>
		<label for="id" data-error="wrong" data-success="right"  >Id</label>
	  </div>

	<div class="md-form mb-5">
	    <i class="fas fa-storer prefix grey-text" id="branch_icon" style="display:none"></i>
		<input type="text"  id="branch_id" style="display:none" name="numOfBranch" class="form-control validate" required>
		<label for="numOfBranch" id="branch_label" style="display:none" data-error="wrong" data-success="right"  >Branch</label>
	</div>	   
	<div class="md-form mb-5">
	    <i class="fas fa-lock prefix grey-text"></i>
		<input type="password" id="password1" name="psw" class="form-control validate" required>
		<label for="psw" data-error="wrong" data-success="right"  >Password</label>
	</div>
	<div class="md-form mb-5">	
		<i class="fas fa-users prefix grey-text"></i>
		<select id="type" class="browser-default custom-select" style="width:400px; margin-left:40px" onchange="onChangeInListAtAddUser()">
					<option value="manager">manager 
					<option value="customer">customer
					<option value="supplier">supplier
					<option value="employee">employee
		</select>	
	</div>
	</div>
	
	
	<div class="modal-footer d-flex justify-content-center">
      <button onclick="addUserToList()"  data-toggle="tooltip" title="One of the fields is incorrect!" type="button" class="btn btn-default" data-dismiss="modal">Add</button>
    </div>
	
	
		
	</div>
	</div>
  </div>
























